   "source": [
    "# Example 2: Using 'continue' to print only even numbers\n",
    "print(\"Printing even numbers between 1 and 10:\")\n",
    "for number in range(1, 11):\n",
    "    if number % 2 != 0:\n",
    "        continue\n",
    "\n",
    "    print(f\"Even number: {number}\")\n",
    "\n",
    "print('Loop finished\\'s.')"
   ]
  }
